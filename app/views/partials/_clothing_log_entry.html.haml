= form_tag '/clothing_logs', :method => :post, :dest => root_path do
  = label_tag 'clothing', 'Clothing: '
  = autocomplete_field_tag 'clothing', '', autocomplete_clothing_name_clothing_index_path, :id_element => '#clothing_id'
  = hidden_field_tag 'clothing_id'
  = label_tag 'date', 'Date: '
  = text_field_tag 'date', Time.now.strftime("%Y-%m-%d")
  = submit_tag 'Save'
