<%= form_tag do %>
  Start: <%= text_field_tag :start, @start.strftime("%Y-%m-%d") %>
  End: <%= text_field_tag :end, @end.strftime("%Y-%m-%d") %>
  <%= submit_tag "Go" %>
<% end %>
<div style="font-size: 20px">
<table>
<% @cat.sort.each do |label|
   list = @time_graphs[label] %> 
<tr><td><%= label %></td><td><span class="sparkline-bar"><%= list.join(',') %></span></tr>
<%   end %>
</table>

</div>
   
